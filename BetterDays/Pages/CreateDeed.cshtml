@page
@model BetterDays.Pages.CreateDeed
@{
    ViewData["Title"] = "Add Deed";
}

<form method="post" class="mb-4">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <div class="form-group mb-3">
        <label asp-for="NewDeed" class="control-label"></label>
        <input asp-for="NewDeed" class="form-control" />
        <span asp-validation-for="NewDeed" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Add Deed</button>
</form>
